{% extends "layouts/main.html" %}

{% set pageName = "Your applications for Falconers Ltd" %}

{% block content %}

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-full">

      {# Notification Banner #}
      {{ govukNotificationBanner({
        titleText: "Reminder",
        html: '<p class="govuk-notification-banner__heading">
                You will not be able to start another application while this application is still being worked on
              </p>
              <p class="govuk-body">
                <a class="govuk-notification-banner__link" href="#">Continue the application</a>
              </p>
              <p class="govuk-body">
                <a class="govuk-notification-banner__link" href="#">Delete this application</a>
              </p>'
      }) }}

      <h1 class="govuk-heading-xl">{{ pageName }}</h1>

      <div class="govuk-button-group">
        {{ govukButton({
          text: "Start new application",
          classes: "govuk-button--executable"
        }) }}
        <p class="govuk-body">You can also start a new application by copying any of your past applications</p>
      </div>

      <div class="govuk-form-group">
        <label class="govuk-label" for="search">Search</label>
        <div class="govuk-input__wrapper">
          <input class="govuk-input govuk-!-width-one-third" id="search" name="search" type="text">
          <button class="govuk-button" style="background-color: #1d70b8;">
            <span class="govuk-visually-hidden">Search</span>
            {# Simple SVG for the magnifying glass #}
            <svg class="govuk-button__start-icon" xmlns="http://www.w3.org/2000/svg" width="17.5" height="19" viewBox="0 0 33 40" aria-hidden="true" focusable="false" fill="white">
              <path d="M25.7 20.045l7.3 7.3-5.5 5.5-7.3-7.3c-2.3 1.5-5.1 2.4-8.2 2.4C5.4 28 0 22.6 0 15.9S5.4 3.8 12.1 3.8s12.1 5.4 12.1 12.1c0 1.5-.3 3-.8 4.145zM12.1 7.4c-4.7 0-8.5 3.8-8.5 8.5s3.8 8.5 8.5 8.5 8.5-3.8 8.5-8.5-3.8-8.5-8.5-8.5z"></path>
            </svg>
          </button>
          <p class="govuk-body govuk-!-margin-left-4" style="display:inline-block; vertical-align: middle;">
            <a href="#" class="govuk-link">Clear search</a>
          </p>
        </div>
      </div>

    </div>
  </div>

  <hr class="govuk-section-break govuk-section-break--m govuk-section-break--visible">

  <div class="govuk-grid-row">
    {# Filters Sidebar #}
    <div class="govuk-grid-column-one-quarter">
      <h2 class="govuk-heading-l">Filters</h2>

      {{ govukCheckboxes({
        idPrefix: "permit-type",
        name: "permit-type",
        classes: "govuk-checkboxes--small",
        fieldset: {
          legend: { text: "Permit type", classes: "govuk-fieldset__legend--s" }
        },
        items: [
          { value: "import", text: "Import" },
          { value: "export", text: "Export" },
          { value: "re-export", text: "Re-export" },
          { value: "article-10", text: "Article 10" }
        ]
      }) }}

      {{ govukCheckboxes({
        idPrefix: "status",
        name: "status",
        classes: "govuk-checkboxes--small",
        fieldset: {
          legend: { text: "Status", classes: "govuk-fieldset__legend--s" }
        },
        items: [
          { value: "in-progress", text: "In progress" },
          { value: "awaiting-payment", text: "Awaiting payment" },
          { value: "awaiting-additional", text: "Awaiting additional payment" },
          { value: "closed", text: "Closed" }
        ]
      }) }}
    </div>

    {# Results Table matching image exactly #}
